          <div class="price">
            <div><h5>Prix</h5></div>
            <div><h4>740 <i class="fas fa-euro-sign"></i></h4></div>
          </div>
          <div class="angle"><i class="fas fa-angle-right"></i></div>
        </div>
      </div>
      <div class="col-xs-12">
        <div class="list-element">
          <div class="list-image"><%= image_tag 'fleuriste.png', class:'small-picture' %></div>
          <div class="list-title"><h3>Bergamotte</h3></div>
          <div class="price">
            <div><h5>Prix</h5></div>
            <div><h4>670 <i class="fas fa-euro-sign"></i></h4></div>
          </div>
          <div class="angle"><i class="fas fa-angle-right"></i></div>
        </div>
      </div>
      <div class="col-xs-12">
        <div class="list-element last">
          <div class="list-image"><%= image_tag 'animations.png', class:'small-picture' %></div>
          <div class="list-title"><h3>Tom le magicien</h3></div>
          <div class="price">
            <div><h5>Prix</h5></div>
            <div><h4>1150 <i class="fas fa-euro-sign"></i></h4></div>
          </div>
          <div class="angle"><i class="fas fa-angle-right"></i></div>
        </div>
      </div>
