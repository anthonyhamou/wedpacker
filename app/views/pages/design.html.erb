<div class="container">
  <div>
    <h1>Buttons</h1>
    <div class="btn-validate"><h4 style="font-weight: 700">Book now</h4></div>
    <div class="btn-validate">Book now</div>
    <div class="btn-validate" id="lock"><i class="fas fa-lock-open"></i></div>
  </div>

  <div>
    <h1>Form</h1>
    <form>
      <input type="text" name="date" placeholder="Quand ?" class="styled-form"><br>
      <input type="text" name="location" placeholder="Où ?" class="styled-form">
    </form>
  </div>


  <div>

    <h1>fil d'ariane</h1>
    <div class="thread">
      <div class="b-box b-box--dots">
        <span><%= link_to "", new_bundle_path, class: "b-box--dots__item is-active" %></span>
        <span class="b-box--dots__item"></span>
        <span class="b-box--dots__item"></span>
        <span class="b-box--dots__item"></span>
        <span class="b-box--dots__item big">Total Price</span>
      </div>

      <div class="list-items">
        <div class="item">Recherche</div>
        <div class="item"></div>
        <div class="item"></div>
        <div class="item"></div>
      </div>
    </div>



    <div class="b-box b-box--dots">
      <span><%= link_to "", class: "b-box--dots__item is-active" %></span>
      <span><%= link_to "", class: "b-box--dots__item is-active" %></span>
      <span class="b-box--dots__item"></span>
      <span class="b-box--dots__item"></span>
      <span class="b-box--dots__item big">Total Price</span>
    </div>

    <div class="list-items">
      <div class="item">Recherche</div>
      <div class="item">Lieu</div>
      <div class="item"></div>
      <div class="item"></div>
    </div>


    <div class="b-box b-box--dots">
      <span class="b-box--dots__item is-active">   </span>
      <span class="b-box--dots__item is-active"></span>
      <span class="b-box--dots__item is-active"></span>
      <span class="b-box--dots__item"></span>
      <span class="b-box--dots__item big">Total Price</span>
    </div>

    <div class="list-items">
      <div class="item">Recherche</div>
      <div class="item">Lieu</div>
      <div class="item">Services</div>
      <div class="item"></div>
    </div>

    <div class="b-box b-box--dots">
      <span class="b-box--dots__item is-active"></span>
      <span class="b-box--dots__item is-active"></span>
      <span class="b-box--dots__item is-active"></span>
      <span class="b-box--dots__item is-active"></span>
      <span class="b-box--dots__item big">Total Price</span>
    </div>

    <div class="list-items">
      <div class="item-last">Recherche</div>
      <div class="item-last">Lieu</div>
      <div class="item-last">Services</div>
      <div class="item-last">Récapitulatif</div>
    </div>

  </div>




  <div>
    <h1>cartes à pictos</h1>
    <div class="service-cards">
      <div class="service-card" id="location">
        <div>
          <%= image_tag 'lieu.png', class:'big-picture' %>
          <h4>Lieu</h4>
        </div>
      </div>
      <div class="service-card" id="food">
        <div>
          <%= image_tag 'traiteur.png', class:'big-picture' %>
          <h4>Traiteur</h4>
        </div>
      </div>
      <div class="service-card" id="music">
        <div>
          <%= image_tag 'musique.png', class:'big-picture' %>
          <h4>Musique</h4>
        </div>
      </div>
      <div class="service-card" id="photo">
        <div>
          <%= image_tag 'photographe.png', class:'big-picture' %>
          <h4>Photographe</h4>
        </div>
      </div>
      <div class="service-card" id="flower">
        <div>
          <%= image_tag 'fleuriste.png', class:'big-picture' %>
          <h4>Fleuriste</h4>
        </div>
      </div>
      <div class="service-card" id="animation">
        <div>
          <%= image_tag 'animations.png', class:'big-picture' %>
          <h4>Animations</h4>
        </div>
      </div>
    </div>
    <div class="service-cards">
      <div class="chosen-service" id="location">
        <div>
          <i class="far fa-check-circle"></i>
          <h4>Lieu ajouté à votre liste service.</h4>
        </div>
      </div>
      <div class="chosen-service" id="food">
        <div>
          <i class="far fa-check-circle"></i>
          <h4>Traiteur ajouté à votre liste service.</h4>
        </div>
      </div>
      <div class="chosen-service" id="music">
        <div>
          <i class="far fa-check-circle"></i>
          <h4>Musique ajouté à votre liste service.</h4>
        </div>
      </div>
      <div class="chosen-service" id="photo">
        <div>
          <i class="far fa-check-circle"></i>
          <h4>Photographe ajouté à votre liste service.</h4>
        </div>
      </div>
      <div class="chosen-service" id="flower">
        <div>
          <i class="far fa-check-circle"></i>
          <h4>Fleuriste ajouté à votre liste service.</h4>
        </div>
      </div>
      <div class="chosen-service" id="animation">
        <div>
          <i class="far fa-check-circle"></i>
          <h4>Animations ajoutées à votre liste service.</h4>
        </div>
      </div>
    </div>
  </div>
  <div>
    <h1>cartes de prestations</h1>
    <div class="card-supplier">
     <%= link_to "#" do %>
       <div class="card-supplier-image" style="background-image: linear-gradient(rgba(0,0,0,0.2), rgba(0,0,0,0.2)),
           url('<%= cl_image_path "jwu6iqplk4pud47gikn0", crop: :fill %>')" %>
           <i class="fas fa-lock-open"></i>
        </div>
      <% end %>
      <div class="card-supplier-footer">
        <h3>NOM DE LA PRESTATION</h3>
        <p class="card-rating">Rating</p>
        <p class="card-price" >5000€</p>
        <%= link_to "+ Détails", "", class: "card-details" %>
      </div>
    </div>
  </div>
  <div>
    <h1>sliders</h1>
    <div class="slidecontainer">
      <input type="range" min="1" max="100" value="50" class="slider" id="myRange">
    </div>
  </div>
  <div>
